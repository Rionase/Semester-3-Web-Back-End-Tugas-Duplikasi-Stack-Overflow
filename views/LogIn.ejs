<!DOCTYPE html>
<!-- saved from url=(0092)https://stackoverflow.com/users/login?ssrc=head&returnurl=https%3a%2f%2fstackoverflow.com%2f -->
<html class="html__responsive html__unpinned-leftnav" lang="en">

<head>


    <title>Log In - Stack Overflow</title>
    
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, minimum-scale=1.0">
    
    <link rel="stylesheet" type="text/css" href="../css/stacks.css">
    <link rel="stylesheet" type="text/css" href="../css/primary.css">

</head>

<body class="universal-auth-page unified-theme floating-content">
    <div id="notify-container"></div>
    <div id="custom-header"></div>

    <header class="s-topbar ps-fixed t0 l0 js-top-bar">
        <div class="s-topbar--container">
            <div class="topbar-dialog leftnav-dialog js-leftnav-dialog dno"
                style="display: none; top: 48px; left: 127.6px;">
                <div class="left-sidebar js-unpinned-left-sidebar" data-is-here-when="sm md lg" id="left-sidebar">
                    <div class="left-sidebar--sticky-container js-sticky-leftnav"></div>
                </div>
            </div>

            <!-- LINK HREF 1 -->
            <a href="/" class="s-topbar--logo js-gps-track"
                data-gps-track="top_nav.click({is_current:true, location:1, destination:8})">
                <span class="-img _glyph">Stack Overflow</span>
            </a>

            <form id="search" role="search" action="https://stackoverflow.com/search"
                class="s-topbar--searchbar js-searchbar " autocomplete="off">
                <div class="s-topbar--searchbar--input-group">
                    <input name="q" type="text" role="combobox" placeholder="Search…" value="" autocomplete="off"
                        maxlength="240" class="s-input s-input__search js-search-field " aria-label="Search"
                        aria-controls="top-search" data-controller="s-popover" data-action="focus-&gt;s-popover#show"
                        data-s-popover-placement="bottom-start" aria-expanded="false">
                    <svg aria-hidden="true" class="s-input-icon s-input-icon__search svg-icon iconSearch" width="18"
                        height="18" viewBox="0 0 18 18">
                        <path
                            d="m18 16.5-5.14-5.18h-.35a7 7 0 1 0-1.19 1.19v.35L16.5 18l1.5-1.5ZM12 7A5 5 0 1 1 2 7a5 5 0 0 1 10 0Z">
                        </path>
                    </svg>

                </div>
            </form>

            <nav class="h100 ml-auto overflow-x-auto pr12">
                <ol class="s-topbar--content" role="menubar">
                    <li role="none">
                        <!-- LINK HREF 2 -->
                        <a href="/LogIn" class="s-topbar--item s-topbar--item__unset s-btn s-btn__filled ws-nowrap js-gps-track" role="menuitem">Log in</a>
                    </li>
                    <li role="none">
                        <!-- LINK HREF 3 -->
                        <a href="/SignUp" class="s-topbar--item s-topbar--item__unset ml4 s-btn s-btn__primary ws-nowrap" role="menuitem">Sign up</a></li>
                </ol>
            </nav>

        </div>
    </header>

    <div class="container">

        <div data-is-here-when="" class="left-sidebar js-pinned-left-sidebar ps-relative" data-can-be="left-sidebar">
        </div>

        <div id="content" class="d-flex flex__center snippet-hidden">

            <div class="flex--item">

                <!-- Log In Pop Up -->
                <% if (status) { %>
                    <div class="val-message val-info">
                        <p class="val-textemphasis">You must be logged in to access this feature.</p>
                        <p>Log in below or <a href="/SignUp">sign up</a></p>
                    </div>
                <% } %>

                <div class="ta-center fs-title mx-auto mb24">
                    <a class="s-topbar--logo js-gps-track" data-gps-track="top_nav.click({is_current:true, location:1, destination:8})">
                        <img src="/image/logo-stackoverflow.png" width="250px">
                    </a>
                </div>

                <br>

                <div id="formContainer" class="mx-auto mb24 p24 wmx3 bg-white bar-lg bs-xl mb24">
                    <form id="login-form" class="d-flex fd-column gs12 gsy">

                        <div class="d-flex fd-column gs4 gsy js-auth-item ">
                            <label class="flex--item s-label" for="email">Email</label>
                            <div class="d-flex ps-relative">

                                <!-- EMAIL INPUT -->
                                <input class="s-input" id="email" type="email" size="30" maxlength="100" name="email">

                                <!-- EMAIL ERROR LOGO -->
                                <svg class="s-input-icon js-alert-icon svg-icon iconAlertCircle" width="18"
                                    height="18" viewBox="0 0 18 18" style="color: var(--red-400); display: none;" id="email-logo">
                                    <path
                                        d="M9 17c-4.36 0-8-3.64-8-8 0-4.36 3.64-8 8-8 4.36 0 8 3.64 8 8 0 4.36-3.64 8-8 8ZM8 4v6h2V4H8Zm0 8v2h2v-2H8Z">
                                    </path>
                                </svg>
                            </div>

                            <!-- EMAIL ERROR MESSAGE -->
                            <p class="flex--item s-input-message js-error-message" style="color: var(--red-400); display: none;" id="email-error">
                                The email or password is incorrect.
                            </p>

                        </div>

                        <div class="d-flex fd-column gs4 gsy js-auth-item ">
                            <label class="flex--item s-label" for="email">Password</label>
                            <div class="d-flex ps-relative">

                                <!-- PASSWORD INPUT -->
                                <input class="s-input" id="password" type="password" size="30" maxlength="100" name="password" >

                                <!-- PASSWORD ERROR LOGO -->
                                <svg class="s-input-icon js-alert-icon svg-icon iconAlertCircle" width="18"
                                    height="18" viewBox="0 0 18 18" style="color: var(--red-400); display: none;" id="password-logo">
                                    <path
                                        d="M9 17c-4.36 0-8-3.64-8-8 0-4.36 3.64-8 8-8 4.36 0 8 3.64 8 8 0 4.36-3.64 8-8 8ZM8 4v6h2V4H8Zm0 8v2h2v-2H8Z">
                                    </path>
                                </svg>
                            </div>

                            <!-- PASSWORD ERROR MESSAGE -->
                            <p class="flex--item s-input-message js-error-message" style="color: var(--red-400); display: none;" id="password-error">
                                Password cannot be empty.
                            </p>

                        </div>




                        <div class="d-flex gs4 gsy fd-column js-auth-item ">
                            <!-- LINK HREF 4 -->
                            <button class="flex--item s-btn s-btn__primary" id="LogInButton" type="button" >Log in</button>
                        </div>


                    </form>
                </div>

                <div class="mx-auto ta-center fs-body1 p16 pb0 mb24 w100 wmx3">
                    Don’t have an account? 
                    <!-- LINK HREF 5 -->
                    <a href="/SignUp">Sign up</a>
                </div>
            </div>

        </div>
    </div>

    <div id="onetrust-consent-sdk" class="d-none"></div>
    <div id="onetrust-banner-sdk" data-controller="s-modal"></div>
    <div id="ot-pc-content" class="d-none"></div>
    <div id="onetrust-style" class="d-none">&nbsp;</div>
    <div class="d-none js-consent-banner-version" data-consent-banner-version="baseline"></div>


    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="/js/LogIn.js"></script>

</body>

</html>